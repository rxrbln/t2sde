e16me_pc() {
	echo "%%% GTK_CONFIG=$GTK_CONFIG"
	echo "%%% GTK_CFLAGS=`$GTK_CONFIG --cflags`"
	echo "%%% GTK_LIBS=`$GTK_CONFIG --libs`"
	echo "%%% E16=$E16"
	echo "%%% CFLAGS=$CFLAGS"
}

hook_add preconf 5 "e16me_pc"
