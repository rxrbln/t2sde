#!/bin/sh
#
# ROCK Linux: /etc/rc.d/init.d/mpud
#
# Desc: PowerPC Power Management Unig daemon
# Runlevel: 70 rcX
#

main_begin

    block_begin(start, `Starting mpud.')
        check(`/sbin/pmud')
    block_end

    block_begin(stop, `Stopping pmud.')
        check(`killall -15 pmud')
    block_end

main_end

