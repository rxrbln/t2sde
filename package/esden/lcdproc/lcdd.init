#!/bin/sh
#
# --- ROCK-COPYRIGHT-NOTE-BEGIN ---
# --- ROCK-COPYRIGHT-NOTE-END ---
#
# Desc: matrix character lc display server
# Runlevel: 25 rcX
#

main_begin

	block_begin(start, `Starting LCDd.')
		check(`D_prefix/sbin/LCDd -c /etc/LCDd.conf &')
	block_end

	block_begin(stop, `Stopping LCDd.')
		check(`killall -15 LCDd')
	block_end

main_end
