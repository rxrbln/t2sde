
We have to filter out some files since net-tools or other packags provide
better versions.

  - Rene Rebe <rene@exactcode.de>

diff -ur coreutils-5.2.1-orig/man/Makefile.in coreutils-5.2.1/man/Makefile.in
--- coreutils-5.2.1-orig/man/Makefile.in	2004-03-11 09:58:00.000000000 +0100
+++ coreutils-5.2.1/man/Makefile.in	2004-10-09 14:05:29.349787440 +0200
@@ -275,6 +275,8 @@
   tty.1 uname.1 unexpand.1 uniq.1 unlink.1 uptime.1 users.1 vdir.1 wc.1 \
   who.1 whoami.1 yes.1
 
+dist_man_MANS := $(filter-out groups.1 hostname.1 su.1 uptime.1,$(dist_man_MANS))
+
 man_aux = $(dist_man_MANS:.1=.x)
 EXTRA_DIST = $(man_aux) help2man
 MAINTAINERCLEANFILES = $(man_MANS)
diff -ur coreutils-5.2.1-orig/src/Makefile.in coreutils-5.2.1/src/Makefile.in
--- coreutils-5.2.1-orig/src/Makefile.in	2004-03-11 09:59:23.000000000 +0100
+++ coreutils-5.2.1/src/Makefile.in	2004-10-09 14:04:54.837034176 +0200
@@ -817,6 +817,11 @@
 sysconfdir = @sysconfdir@
 target_alias = @target_alias@
 bin_SCRIPTS = groups
+
+# some filtering
+bin_PROGRAMS := $(filter-out su$(EXEEXT) uptime$(EXEEXT) hostname$(EXEEXT),$(bin_PROGRAMS))
+bin_SCRIPTS := $(filter-out groups,$(bin_SCRIPTS))
+
 noinst_HEADERS = \
   system.h checksum.h copy.h cp-hash.h ls.h dircolors.h remove.h \
   chown-core.h fs.h \
