#!/bin/sh
#
# ROCK Linux: /etc/rc.d/init.d/kdm
#
# Desc: KDE Display Manager
# Runlevel: 80 rcX
#

main_begin

    block_begin(start, `Starting kdm.')
        check(`D_prefix/bin/kdm')
    block_end

    block_begin(stop, `Stopping kdm.')
        check(`killall -15 D_prefix/bin/kdm')
    block_end

main_end

