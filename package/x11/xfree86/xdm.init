#!/bin/sh
#
# ROCK Linux: /etc/rc.d/init.d/xdm
#
# Desc: X Display Manager
# Runlevel: 80 rcX
#

main_begin

    block_begin(start, `Starting xdm.')
        check(`/usr/X11/bin/xdm')
    block_end

    block_begin(stop, `Stopping xdm.')
        check(`killall -15 /usr/X11/bin/xdm')
    block_end

main_end

exit 0
