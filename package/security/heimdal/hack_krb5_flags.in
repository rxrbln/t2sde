#!/bin/sh

KRB5LIBS="`krb5-config --libs gssapi` -lhdb -lkadm5clnt -lkadm5srv"
KRB5CFLAGS=`krb5-config --cflags gssapi`

export LIBS="$LIBS $KRB5LIBS"
export LDFLAGS="$LDFLAGS $KRB5LIBS"

export CFLAGS="$CFLAGS $KRB5CFLAGS"
export CXXFLAGS="$CXXFLAGS $KRB5CFLAGS"
export CPPFLAGS="$CPPFLAGS $KRB5CFLAGS"

