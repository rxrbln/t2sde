#!/bin/sh

KRB5LIBS="`krb5-config --libs gssapi` -lhdb -lkadm5clnt -lkadm5srv"
KRB5CFLAGS=`krb5-config --cflags gssapi`

export LIBS=`echo $LIBS $KRB5LIBS`
export LDFLAGS=`echo $LDFLAGS $KRB5LIBS`

var_append CPP_WRAPPER_APPEND ' ' "$KRB5CFLAGS"
var_append CXX_WRAPPER_APPEND ' ' "$KRB5CFLAGS"
var_append GCC_WRAPPER_APPEND ' ' "$KRB5CFLAGS"

