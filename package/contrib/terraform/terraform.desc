[F] CROSS
[R] + x86 x86-64 arm arm64 ppc64le mips mips64 mipsel

[L] MPL
[S] Stable
[V] 1.11.0
[P] X -----5---9 160.000

[O] . $base/package/*/*/go-conf.in
[O] export GOARCH=$TARGET_ARCH GOOS=$SDECFG_KERNEL
[O] var_append goconfopt ' ' "-o bin/$pkg"
[O] hook_add postmake 5 "cp bin/$pkg $root/$bindir"

