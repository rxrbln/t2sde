#!/bin/sh
# 
# ROCK Linux: /etc/init.d/smbmount
#
# Desc: mounting and unmounting for Windows shares
# Runlevel: 75 rcX
#

main_begin

    block_begin(start, `Starting SAMBA server.')
        check(`mount -av -t smbfs')
    block_end

    block_begin(stop, `Stopping squid proxy server.')
       check(`umount -av -t smbfs')
    block_end

main_end
