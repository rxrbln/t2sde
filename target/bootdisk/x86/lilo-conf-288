disk=/dev/floppy/0
	bios = 0x00
	sectors = 36
	heads = 2
	cylinders = 80

nowarn
boot=/dev/floppy/0
install=/mnt/boot.b
map=/mnt/map
vga=normal
message=/mnt/lilo-help.txt
geometric
compact
prompt

image=/mnt/vmlinuz
	read-write
	append="root=/dev/ram devfs=nocompat init=/linuxrc"
	initrd=/mnt/initrd.gz
	label=ramdisk

image=/mnt/vmlinuz
	label=linux

image=/mnt/memtest.bin
	label=memtest
	optional

