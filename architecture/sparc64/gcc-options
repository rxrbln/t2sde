
if [ "$ROCKCFG_SPARC_OPT" ] ; then
	opt=""
	case "$ROCKCFG_SPARC_OPT" in
		v7) opt="-mcpu=cypress -mtune=cypress" ;;
		v8) opt="-mcpu=supersparc -mtune=supersparc" ;;
		v9) opt="-mcpu=ultrasparc -mtune=ultrasparc -mno-app-regs" ;;
		v9vis) opt="-mcpu=ultrasparc -mtune=ultrasparc -mvis -mno-app-regs" ;;
	esac
	var_append GCC_WRAPPER_INSERT " " "$opt" 
fi

