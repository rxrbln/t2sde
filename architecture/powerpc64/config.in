choice ROCKCFG_POWERPC64_OPT generic \
	generic		"No special optimisation"			\
	power3		"Optimised for Power3"				\
	power4		"Optimised for Power4"				\
	power5		"Optimised for Power5"				\
	g5		"Optimised for G5"

if [ $ROCKCFG_POWERPC64_OPT != generic ] ; then
	ROCKCFG_ID="$ROCKCFG_ID-$ROCKCFG_POWERPC64_OPT"
fi

ROCKCFGSET_MULTILIB=1

