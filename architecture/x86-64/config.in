#Description: x86-64 (AMD64, EM64T)

choice SDECFG_X8664_OPT generic   \
	generic		"No special optimisation"		\
	k8		"Optimised for generic K8"		\
	opteron		"Optimised for AMD Opteron"		\
	athlon64	"Optimised for AMD Athlon64"		\
	athlon-fx	"Optimised for AMD Athlon-FX"		\
	nocona		"Optimised for Intel EM64T (Pentium 4, Xeon, Core2)"

bool 'Optimise for the Xen hypervisor' SDECFG_X8664_OPT_XEN 0

if [ $SDECFG_X8664_OPT != generic ] ; then
	SDECFG_ID="$SDECFG_ID-$SDECFG_X8664_OPT"
fi

SDECFGSET_MULTILIB=1
