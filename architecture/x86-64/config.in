choice ROCKCFG_X8664_OPT generic   \
	generic		"No special optimisation"		\
	k8		"Optimised for generic K8"		\
	opteron		"Optimised for AMD Opteron"		\
	athlon64	"Optimised for AMD Athlon64"		\
