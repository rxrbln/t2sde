if [ $SDECFG_IA64_OPT != generic ]; then
	var_append GCC_WRAPPER_INSERT " " "-mtune=$SDECFG_IA64_OPT"
	var_append KCC_WRAPPER_APPEND " " "-mtune=$SDECFG_IA64_OPT"
fi

# stack protection is unsupported on ia64
var_append GCC_WRAPPER_REMOVE ' ' '-fstack-protector'
var_append GCC_WRAPPER_REMOVE ' ' '-fstack-protector-strong'
var_append GCC_WRAPPER_REMOVE ' ' '-fstack-clash-protection'
