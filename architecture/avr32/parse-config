case $pkg in
	linux)
		:
		;;
	*)
		var_append CC_WRAPPER_INSERT ' ' '-std=gnu99'
		;;
esac

case $pkg in
	glib)
		var_append GCC_WRAPPER_REMOVE ' ' '-Werror=missing-prototypes'
		;;
	sysvinit)
		var_append GCC_WRAPPER_REMOVE ' ' '-fstack-protector-strong'
		;;
	procps)
		var_append GCC_WRAPPER_REMOVE ' ' '-Wno-unused-result'
		;;
esac
