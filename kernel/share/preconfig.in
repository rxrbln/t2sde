CFGTEMP_KERNELLIST=
for x in kernel/*; do
#	pkgout
	if [ -d $x -a $x != kernel/share ]; then
		y="`grep '^#Description: ' $x/config.in 2> /dev/null |
			head -n 1 | cut -d' ' -f2- | tr ' ' '_'`"
		[ -n "$y" ] || y=`echo ${x#kernel/} | tr 'a-z' 'A-Z'`
		CFGTEMP_KERNELLIST="$CFGTEMP_KERNELLIST ${x#kernel/} $y"
	fi
#	pkgin
done
